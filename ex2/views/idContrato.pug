extends layout 

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1 Contrato #{contrato._id}

        .w3-container
            ul.w3-ul.w3-card-4(style="width:50%")
                li
                    b ID:
                    span= contrato._id
                li
                    b Número do anúncio:
                    span= contrato.nAnuncio
                li
                    b Procedimento:
                    span= contrato.tipoprocedimento
                li
                    b Objeto Contrato:
                    span= contrato.objectoContrato
                li
                    b Data de publicação:
                    span= contrato.dataPublicacao
                li
                    b Data de celebração do contrato:
                    span= contrato.dataCelebracaoContrato
                li
                    b Preço contratual:
                    span= contrato.precoContratual
                li
                    b Prazo de execução:
                    span= contrato.prazoExecucao
                li
                    b NIPC - Entidade comunicante:
                    span= contrato.NIPC_entidade_comunicante
                li
                    b Entidade comunicante:
                    span= contrato.entidade_comunicante
                li
                    b Fundamentação:
                    span= contrato.fundamentacao
        footer.w3-container.w3-purple 
            h5 
                span Generated by EngWeb2024 in #{data} - [
                    a(href="/") Return
                span  ] 